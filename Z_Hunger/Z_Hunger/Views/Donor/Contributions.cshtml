
@{
    ViewBag.Title = "Contributions";
    Layout = "~/Views/Shared/_DonorLayout.cshtml";
}

<h3>Contributions List</h3>
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Food Description</th>
        <th>Preserve Time</th>
        <th>Status</th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FoodDes</td>
                <td>@item.PreTime</td>
                <td>@item.Status</td>
            </tr>
        }
    }

</table>

